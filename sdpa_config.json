{
    "experiment_type": "SDPA_INNOVATION",
    "torch_optimizations": {
        "cudnn_benchmark": true,
        "allow_tf32_matmul": true,
        "allow_tf32_cudnn": true
    },
    "environment_variables": {
        "PYTORCH_CUDA_ALLOC_CONF": "expandable_segments:True",
        "OMP_NUM_THREADS": "6"
    },
    "training_config": {
        "epochs": 20,
        "batch": 8,
        "imgsz": 640,
        "device": "cuda:0",
        "workers": 6,
        "optimizer": "AdamW",
        "lr0": 0.001,
        "amp": false,
        "cache": false,
        "patience": 30,
        "save_period": 5
    }
}
